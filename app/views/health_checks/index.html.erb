<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    <% @health_checks.each do |health_check| %>
      <tr>
        <td><%= link_to health_check.name, site_health_check_path(@site, health_check) %></td>
        <td><%= link_to 'Edit', edit_site_health_check_path(@site, health_check) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Health Check', new_site_health_check_path(@site) %>
