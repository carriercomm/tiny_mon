<% title t('.all_accounts') %>

<div class="box">
  <div class="box-body">
    <% if @accounts.empty? %>
      <p><%= t('.no_accounts') %></p>
    <% else %>
      <%= render :partial => "/shared/filter", :locals => { :url => admin_accounts_path } %>
  
      <div id="accounts">
        <%= render :partial => "index" %>
      </div>
    <% end %>
  </div>
</div>
